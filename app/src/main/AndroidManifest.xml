<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.blackboxembedded.WunderLINQLauncher">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoTitleBar">
        <activity
            android:name="com.blackboxembedded.WunderLINQLauncher.AboutActivity"
            android:label="@string/title_activity_about"
            android:theme="@style/AppTheme.NoTitleBar.NoActionBar"></activity>
        <activity
            android:name="com.blackboxembedded.WunderLINQLauncher.HomeActivity"
            android:label="@string/app_name"
            android:launchMode="singleTask"
            android:stateNotNeeded="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.HOME" />
                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
